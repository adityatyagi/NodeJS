<html ng-app="TestApp">
<!--This is the name of my angular app = TestApp -->

<head>
    <title>The MEAN Stack</title>
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.6.5/angular.js">
    </script>
</head>

<body ng-controller="MainController as vm">
    <input type="text" ng-model="vm.message"><br>
    <!-- vm = View Model -->
    {{ vm.message }}
    <!-- replace this with the value of this.message -->
    <br>
    <ul>
        <li ng-repeat="person in vm.people">
            {{ person.name}}
        </li>
    </ul>
    <script src="/assets/js/app.js"></script>

</body>

</html>